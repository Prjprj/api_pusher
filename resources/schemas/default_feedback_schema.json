{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Feedback",
  "type": "object",
  "properties": {
    "username": {
      "type": "string"
    },
    "feedback_date": {
      "type": "string",
      "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
    },
    "campaign_id": {
      "type": "string"
    },
    "comment": {
      "type": "string"
    }
  },
  "required": [
    "username",
    "feedback_date",
    "campaign_id",
    "comment"
  ]
}